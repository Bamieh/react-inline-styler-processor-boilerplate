{
  "name": "react-inline-styler-processor-rtl",
  "version": "0.1.0",
  "description": "Boilerplate for creating React Inline Styler Processors.",
  "main": "./distribution/index.js",
  "scripts": {
    "build": "babel source --out-dir distribution",
    "test": "mocha --require test/setup.js test/**/*.test.js --compilers js:babel-core/register --recursive",
    "test-continuous": "mocha --require test/setup.js test/**/*.test.js --compilers js:babel-core/register --recursive --watch",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-node5": "^11.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "mocha": "^3.2.0",
    "object.entries": "^1.0.4",
  },
  "author": "Ahmad Bamieh",
  "license": "MIT"
}
